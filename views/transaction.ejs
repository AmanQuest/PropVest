<%- include('./ellington-new/header.ejs') %>

 

<section class="main-banner about-banner project-sec-inner transaction-page">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-sm-10 m-auto transaction-inner">
              <h2 class="main-heading-transaction">Transaction List</h2>
                <div class="row">
                    <div class="col-sm-12">
                        <div>
                            <div class="row">
                                <div class="col-md-5">
                                    <div>
                                        <div class="search-field">
                                        <div class="input-group">
                                            <div class="input-group-prepend bg-transparent">
                                              <i class="input-group-text  fa fa-search" aria-hidden="true"></i>
                                            </div>
                                            <input type="text" class="form-control bg-transparent" placeholder="Search......">
                                          </div>
                                      </div>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div>
                                        <div class="search-field">
                                        <div class="input-group">
                                            <div class="input-group-prepend bg-transparent">
                                              <div class="input-group-text ">
                                                  Filter
                                              </div>
                                            </div>
                                            <select class="form-control bg-transparent">
                                               <option>Sort By</option>
                                               <option>Sort By 1</option>
                                               <option>Sort By 2</option>
                                               <option>Sort By 3</option>
                                             </select>
                                           
                                          </div>
                                      </div>
                                    </div>
                                    
                                </div>
                            </div>
                        </div>
                        <div class="table-responsive">
                            <table class="table table-striped table-bordered table-transaction">
                            <thead>
                                <tr>
                                    <th width="8%">Image</th>
                                    <th width="14%">Plot Type</th>
                                    <th width="8%">Amount</th>
                                    <th width="20%">Payment hash</th>
                                    <th width="20%">Token Id</th>
                                    <th width="20%">Wallet address To</th>
                                    <th width="10%">Status</th>
                                </tr>
                            </thead>
                            <tbody>
                                <!-- dynamic data start -->

                                <% if(tx.length>0){
                                    tx.forEach(function(value,index){%>

                                    <tr>
                                        <td><img  width="50" src="/uploadFile/<%=value.landInfo.image %>" alt=""></td>
                                        <td><% if(value.landInfo){%> 
                                            <%=value.landInfo.name %>
                                            <%} %></td>
                                        <td><%=value.total %> Matic</td>

                                        <!-- hash -->
                                        <td><a id="hash" href="https://mumbai.polygonscan.com//tx/<%=value.hash %>" target="_blank"><%=value.hash %></a></td>

                                        <!-- wallet address from -->
                                        <td><%=value.tokenId
                                            %></td>
                                        <td><a href="https://mumbai.polygonscan.com//address/<%=value.wallet_address %>" target="_blank"><%=value.wallet_address %></a></td>

                                        <!-- wallet address to -->
                                        <!-- <td><a href="https://mumbai.polygonscan.com//address/<%=value.address_to %>" target="_blank"><%=value.address_to %></a></td> -->

                                        <!-- status -->
                                        <td><span class="badge badge-success">Success </span> </td>

                                    </tr>
                                    
                                    <tr class="d-none">
                                        <td><img  width="50" src="assets/images/service_img.png" alt=""></td>
                                        <td>Plane House</td>
                                        <td>30,000</td>
                                        <td>43jh43hjhjh3hgg4334</td>
                                        <td>vvb4vb4vb433bbv4n43m4434v34vvb432vb2bv</td>
                                        <td>vvb4vb4vb433bbv4n43m4434v34vvb432vb2bv</td>
                                        <td><span class="badge badge-success">Confirm</span> </td>
                                    </tr>

                                <% }) }%>

                                <!-- dunamic data end -->
                               
                            </tbody>
                        </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
 

<%- include('./ellington-new/footer.ejs') %>
