<%- include('./ellington-new/header.ejs') %>

<section class="main-banner about-banner project-sec-inner add-prop-sec">
    <div class="container">
        <div class="row">
            <div class="col-md-10 m-auto">
                <div class="add-property-box">
                    <div class="w-100">
                        <div class="row">
                        <div class="col-md-6">
                           <div class="form-group">
                            <label>Land Tittle</label>
                            <input type="text" class="form-control" placeholder="Enter Land Tittle">
                          </div> 
                      </div>
                      <div class="col-md-6">
                          <div class="form-group">
                            <label>Land Image</label>
                            <input type="file" class="form-control" placeholder="Enter Land Tittle">
                          </div>
                        </div>
                        
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                            <label>Land Size</label>
                            <input type="text" class="form-control" placeholder="Land Size height*width Sq Ft">
                          </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                            <label>MIN BUYING AMOUNT</label>
                            <input type="text" class="form-control" placeholder="MIN BUYING AMOUNT">
                          </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                            <label>PRICE</label>
                            <input type="text" class="form-control" placeholder="PRICE">
                          </div>
                        </div>
                        
                       
                        
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                            <label>QUNATITY PER BRICK</label>
                            <input type="text" class="form-control" placeholder="Enter Quantity">
                          </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                            <label>RATE PER BRICK</label>
                            <input type="text" class="form-control" placeholder="Enter Rate">
                          </div>
                        </div>
                         <div class="col-md-4">
                            <div class="form-group">
                            <label>QUNATITY(No of Plots)</label>
                            <input type="text" class="form-control" placeholder="Enter Quantity(No of Plots)">
                          </div>
                        </div>
                    </div>
                    <div>
                        <button class="common-btn">Submit</button>
                    </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<%- include('./ellington-new/footer.ejs') %>